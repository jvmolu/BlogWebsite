<%- include('partials/header'); -%>
<h1>COMPOSE</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <h3>Title</h3>
    <input class="form-control" type="text" name="Title" autocomplete="off">
  </div>
  <div class="form-group">
    <h3>Post</h3>
    <textarea class="form-control" name="Post" rows="10" cols="30"></textarea>
  </div>
  <button class="btn-primary btn" type="submit" name="button" value="Post">Publish</button>
</form>
<%- include('partials/footer'); -%>
